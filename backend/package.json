{
  "name": "tripgo-backend",
  "version": "1.0.0",
  "description": "Backend API for TripGo travel booking platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "rm -rf dist && npx esbuild src/server.ts src/utils/tenantSeeder.ts --outdir=dist --platform=node --target=node18 --format=cjs --external:@prisma/client --external:bcryptjs --external:jsonwebtoken --external:nodemailer --external:stripe --external:multer --external:sharp --external:cors --external:helmet --external:express --external:compression --external:express-rate-limit --external:mime-types --external:validator --external:zod --external:dotenv",
    "build:fallback": "tsc -p tsconfig.prod.json --noEmitOnError false",
    "build:strict": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/utils/seedAdmin.ts",
    "db:seed:tenants": "node dist/utils/tenantSeeder.js || tsx src/utils/tenantSeeder.ts",
    "db:reset": "prisma migrate reset --force && npm run db:seed:tenants"
  },
  "keywords": [
    "travel",
    "booking",
    "api",
    "typescript",
    "express"
  ],
  "author": "TripGo Team",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@types/compression": "^1.8.1",
    "@types/mime-types": "^3.0.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "sharp": "^0.34.4",
    "stripe": "^14.7.0",
    "validator": "^13.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.9.0",
    "@types/nodemailer": "^6.4.14",
    "@types/validator": "^13.11.6",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prisma": "^5.6.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  }
}
